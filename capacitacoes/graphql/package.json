  "dependencies": {
    "apollo-server": "^2.25.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "firebase": "^8.0.1",
    "graphql": "^15.5.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "lodash": "^4.17.20",
    "merge-graphql-schemas": "^1.7.8",
    "nodemon": "^2.0.6",
    "sqlite3": "^5.0.0",
